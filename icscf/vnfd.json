      {  
         "name":"icscf",
         "vendor":"fokus",
         "version":"6.0.0",
         "lifecycle_event":[  
            {  
               "event":"CONFIGURE",
               "lifecycle_events":[
			"bind9_relation_joined.sh",
			"fhoss_relation_joined.sh",
			"scscf_relation_joined.sh"
               ]
            },
            {  
               "event":"INSTANTIATE",
               "lifecycle_events":[ 
			"icscf_install.sh"
               ]
            },
            {  
               "event":"START",
               "lifecycle_events":[
			"icscf_generate_config.sh",
			"icscf_start.sh" 
               ]
            }
         ],
         "configurations":{  
            "name":"client-configuration",
            "configurationParameters":[
	       {  
                  "confKey":"port",
                  "value":"5060",
                  "description":"The port used for the icscf in the OpenIMSCore"
               },
	       {  
                  "confKey":"name",
                  "value":"icscf",
                  "description":"The name to be used for the icscf for creating the entries for DNS."
               },
	       {  
                  "confKey":"diameter_p",
                  "value":"3869",
                  "description":"The port on which icscf will listen for incoming diameter connections."
               }
            ]
         },
         "vdu":[  
            {  
               "vm_image":[],
               "scale_in_out":1,
               "vnfc":[  
                  {  
                     "connection_point":[  
                        {  
			   "floatingIp":"random",
                           "virtual_link_reference":"mgmt"
                        } 
                     ]
                  }
               ]
            }
         ],
         "virtual_link":[  
            {  
               "name":"mgmt"
            }
         ],
         "deployment_flavour":[  
            {  
               "flavour_key":"m1.small"
            }
         ],
         "type":"icscf",
         "endpoint":"generic",
         "requires":
         {
           "fhoss":
              {
                 "parameters":[
                 	"name",
                        "port",
                        "mgmt",
                        "mgmt_floatingIp"]},
           "bind9":
              {
                 "parameters":[
                 	"useFloatingIpsForEntries",
                        "realm",
                        "mgmt",
                        "mgmt_floatingIp"]},
           "scscf":
              {
                 "parameters":[
                 	"name",
                        "port",
                        "mgmt",
                        "mgmt_floatingIp"]}
         }
      }
